<style>
  .pagina-central {
    max-width: 900px;
    margin: 2.5em auto 0 auto;
    padding: 2.2em 1.5em 2.5em 1.5em;
    background: #fff;
    border-radius: 18px;
    box-shadow: 0 4px 32px 0 rgba(60, 180, 115, 0.10), 0 1.5px 8px 0 rgba(0,0,0,0.04);
    min-height: 60vh;
    position: relative;
    z-index: 1;
    display: flex;
    flex-direction: column;
    align-items: center;
    background: rgba(255,255,255,0.92);
  .pagina-central h1 {
    border-radius: 22px;
    box-shadow: 0 6px 32px 0 rgba(0,207,255,0.10), 0 1.5px 8px 0 rgba(59,178,115,0.08);
    font-family: 'Baloo 2', 'Fredoka', Arial, sans-serif;
    font-size: 2em;
    color: #3bb273;
    font-weight: 800;
    margin-bottom: 1.2em;
    margin-top: 0.5em;
    backdrop-filter: blur(1.5px);
    animation: fadeInUp 0.7s cubic-bezier(.23,1.01,.32,1) both;
    letter-spacing: 0.01em;
  @keyframes fadeInUp {
    0% { opacity: 0; transform: translateY(40px); }
    100% { opacity: 1; transform: translateY(0); }
  }
  }
  .pagina-central p, .pagina-central ul, .pagina-central li, .pagina-central label, .pagina-central input, .pagina-central textarea {
    font-size: 1.13em;
    color: #222;
    font-size: 2.2em;
    line-height: 1.7;
    font-weight: 900;
    text-align: center;
  }
  .pagina-central form {
    background: linear-gradient(90deg, #00CFFF 0%, #FFD600 100%);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
    background-clip: text;
    width: 100%;
    max-width: 420px;
    margin: 0 auto;
    display: flex;
    flex-direction: column;
    gap: 1.1em;
    align-items: center;
  }
  .pagina-central input, .pagina-central textarea {
    font-size: 1.15em;
    width: 100%;
    padding: 0.7em 1em;
    border-radius: 8px;
    border: 1.5px solid #3bb273;
    background: #f8fff9;
    font-size: 1em;
    font-family: 'Fredoka', 'Quicksand', Arial, sans-serif;
    margin-bottom: 0.7em;
    outline: none;
    background: rgba(230,247,238,0.45);
    border-radius: 10px;
    box-shadow: 0 1.5px 8px 0 rgba(0,207,255,0.04);
    padding: 1.2em 1em;
    transition: border 0.18s;
  }
  .pagina-central input:focus, .pagina-central textarea:focus {
    border: 2px solid #00CFFF;
  }
  .pagina-central button[type="submit"] {
    background: linear-gradient(90deg, #00CFFF 0%, #3bb273 100%);
    color: #fff;
    font-weight: 800;
    font-size: 1.08em;
    border: none;
    border-radius: 8px;
    padding: 0.6em 1.5em;
    cursor: pointer;
    box-shadow: 0 2px 8px 0 rgba(0,207,255,0.10);
    transition: background 0.18s, box-shadow 0.18s, transform 0.18s;
    outline: none;
    margin-top: 0.2em;
  }
  .pagina-central button[type="submit"]:hover {
    background: linear-gradient(90deg, #3bb273 0%, #00CFFF 100%);
    box-shadow: 0 4px 16px 0 rgba(0,207,255,0.18);
    transform: scale(1.04);
  }
  @media (max-width: 900px) {
    .pagina-central {
      max-width: 98vw;
      padding: 1.2em 0.2em 2em 0.2em;
    }
  }
  @media (max-width: 600px) {
    .pagina-central {
      margin: 1.2em auto 0 auto;
      padding: 0.7em 0.1em 1.2em 0.1em;
      border-radius: 10px;
    }
    .pagina-central h1 {
      font-size: 1.25em;
    }
  }
</style>
